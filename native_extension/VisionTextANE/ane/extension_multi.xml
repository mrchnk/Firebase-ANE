<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.mlkit.vision.TextANE</id>
    <name>Firebase Vision Text ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.14.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTextANE.a</nativeLibrary>
                <initializer>TRFBVTXExtInizer</initializer>
                <finalizer>TRFBVTXExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.vision.TextANE</initializer>
                <finalizer>com.tuarua.mlkit.vision.TextANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.vision.TextANE</initializer>
                <finalizer>com.tuarua.mlkit.vision.TextANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.vision.TextANE</initializer>
                <finalizer>com.tuarua.mlkit.vision.TextANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
