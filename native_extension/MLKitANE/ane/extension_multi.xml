<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.mlkit.MLKitANE</id>
    <name>MLKit ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2020 Tua Rua Ltd.</copyright>
    <versionNumber>0.15.0</versionNumber>
    <platforms>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.MLKitANE</initializer>
                <finalizer>com.tuarua.mlkit.MLKitANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.MLKitANE</initializer>
                <finalizer>com.tuarua.mlkit.MLKitANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.mlkit.MLKitANE</initializer>
                <finalizer>com.tuarua.mlkit.MLKitANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libMLKitANE.a</nativeLibrary>
                <initializer>TRFBMLKExtInizer</initializer>
                <finalizer>TRFBMLKExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
